<section>
    <h2>Crie sua própria sala</h2>
    <form action="/create-room" method="POST">
        <label class="sr-only" for="room-pass">Insira uma senha</label>
        <input type="password" id="room-pass" name="password" placeholder="Insira uma senha">
        <button class="enter-room">
            <img src="/images/enter-room.svg" alt="Ícone - Entrar em uma sala">
            Criar sala
        </button>
    </form>
</section>